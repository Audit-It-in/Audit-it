{
  "name": "thefinxperts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "next dev",
    "dev:prod": "next dev --env-file=.env.production",
    "build": "next build",
    "build:dev": "next build --env-file=.env.development",
    "build:prod": "next build --env-file=.env.production",
    "start": "serve out",
    "start:dev": "NODE_ENV=development serve out",
    "start:prod": "NODE_ENV=production serve out",
    "lint": "next lint",
    "test": "jest --no-cache --testTimeout=10000",
    "build-deploy": "npm run build && firebase deploy",
    "build-deploy:dev": "npm run build:dev && firebase deploy",
    "build-deploy:prod": "npm run build:prod && firebase deploy",
    "vercel-deploy:dev": "vercel",
    "vercel-deploy:prod": "vercel --prod",
    "test:watch": "jest --watch --no-cache --testTimeout=10000",
    "test:ui": "jest --no-cache --testTimeout=10000 src/tests/ui",
    "test:components": "jest --no-cache --testTimeout=10000 src/tests/components",
    "process-locations": "node scripts/process-location-data.js",
    "process-locations:force": "node scripts/process-location-data.js --force",
    "clear-location-imports": "node scripts/clear-location-imports.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jotai": "^2.12.5",
    "lucide-react": "^0.526.0",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "lightningcss": "^1.28.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
